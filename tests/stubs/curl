#!/usr/bin/env bash
set -euo pipefail
log="${CMD_LOG:-/tmp/cmd.log}"
echo "curl $*" >> "$log"
url=""
outfile=""
while [[ $# -gt 0 ]]; do
  case "$1" in
    -o) outfile="$2"; shift 2 ;;
    -fsSL) shift 1 ;;
    *) url="$1"; shift 1 ;;
  esac
done
if [[ -z "$outfile" ]]; then
  cat
else
  printf "dummy-key" > "$outfile"
fi
